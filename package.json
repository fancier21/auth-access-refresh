  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
